{
  "name": "shongle",
  "version": "1.0.0",
  "description": "Project for tracking some basic market data",
  "main": "src/index",
  "type": "module",
  "imports": {
    "#root/*.js": "./*.js",
    "#root/*.ts": "./*.ts"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "coverage": "jest --coverage",
    "build": "npx tsc",
    "start:prod": "tsx --env-file=.env --import=tsx src/index.ts",
    "start:dev": "nodemon --exec tsx --env-file=.env.dev --import=tsx --ext 'ts,json,js,mjs,cjs' src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycby/shingle.git"
  },
  "author": "ycby",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ycby/shingle/issues"
  },
  "homepage": "https://github.com/ycby/shingle#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "csv": "^6.3.9",
    "eslint": "^9.4.0",
    "express": "^5.2.1",
    "mariadb": "^3.3.1",
    "papaparse": "^5.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/compat": "^1.0.3",
    "@eslint/js": "^9.4.0",
    "@jest/globals": "^30.0.4",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/papaparse": "^5.3.16",
    "babel-jest": "^30.0.4",
    "globals": "^15.4.0",
    "jest": "^30.0.4",
    "nodemon": "^3.1.3",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4"
  }
}
